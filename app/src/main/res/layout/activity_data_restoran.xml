<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="singleclick.taxcollector.DataRestoran">

    <TextView
        android:text="DATA RESTORAN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="22dp"
        android:textAlignment="center"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Space
        android:layout_width="20px"
        android:layout_height="30px"
        android:layout_alignParentStart="true"
        android:id="@+id/space"
        android:layout_below="@+id/textView"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nomor NPWPD"
        android:id="@+id/textView2"
        android:layout_alignParentStart="true"
        android:textSize="18dp"
        android:layout_below="@+id/space"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jenis Restoran"
        android:id="@+id/textView3"
        android:layout_alignParentStart="true"
        android:textSize="18dp"
        android:layout_below="@+id/textView2"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jumlah Meja"
        android:id="@+id/textView4"
        android:layout_alignParentStart="true"
        android:textSize="18dp"
        android:layout_below="@+id/textView3"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jumlah Kursi"
        android:id="@+id/textView5"
        android:layout_alignParentStart="true"
        android:textSize="18dp"
        android:layout_below="@+id/textView4"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rata-rata Tamu/Hari"
        android:id="@+id/textView6"
        android:layout_alignParentStart="true"
        android:textSize="18dp"
        android:layout_below="@+id/textView5"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rata-rata Harga/Porsi"
        android:id="@+id/textView7"
        android:layout_alignParentStart="true"
        android:textSize="18dp"
        android:layout_below="@+id/textView6"
        android:layout_alignParentLeft="true" />

    <Space
        android:layout_width="20px"
        android:layout_height="30px"
        android:layout_alignParentStart="true"
        android:id="@+id/space2"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView7" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/checkBox"
        android:checked="false"
        android:textSize="18dp"
        android:text="Menggunakan Sistem Komputerisasi"
        android:layout_below="@+id/space2"
        android:layout_alignParentLeft="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Menggunakan Kasir"
        android:id="@+id/checkBox2"
        android:textSize="18dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/checkBox" />

    <Space
        android:layout_width="20px"
        android:layout_height="20px"
        android:layout_below="@+id/checkBox2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/space3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jumlah Pegawai"
        android:id="@+id/textView8"
        android:layout_below="@+id/space3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="18dp" />
</RelativeLayout>
